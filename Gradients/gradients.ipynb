    "import tensorflow as tf"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "x = tf.Variable(0.0)\n",
    "\n",
    "for _ in range(200):\n",
    "  \n",
    "  with tf.GradientTape() as tape:\n",
    "    y = x**2 - 6 * x + 5\n",
    "\n",
    "  dy_dx = tape.gradient(y, x)\n",
    "  x.assign_sub(0.1 * dy_dx)\n",
    "\n",
    "  print(x.numpy())\n",
    "  \n"
   ]
  }
